{
  "name": "folder-commands",
  "displayName": "Folder Commands",
  "description": "",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrdutton/vscode-folder-commands.git"
  },
  "keywords": [],
  "publisher": "jrdutton",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:folderCommands.runCommands"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folderCommands.runCommands",
        "title": "Run Commands",
        "category": "Folder Commands"
      }
    ],
    "configuration": {
      "properties": {
        "folderCommands.fileTypes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "typescript"
          ],
          "description": "Specifies the types of files."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "prettier": {
    "printWidth": 150,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
